{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/context/(.*)",
      "dest": "/api/context/[...path].ts"
    },
    {
      "src": "/api/file/(.*)",
      "dest": "/api/file/[file_id].ts?file_id=$1"
    }
  ]
}
