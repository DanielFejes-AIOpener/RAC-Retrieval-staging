{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "explorer.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/explorer",
      "dest": "/explorer.html"
    },
    {
      "src": "/api/rac/([a-z]+)",
      "dest": "/api/rac/[client].ts?client=$1"
    },
    {
      "src": "/api/rac",
      "dest": "/api/rac.ts"
    },
    {
      "src": "/api/context/(.*)",
      "dest": "/api/context/[...path].ts?path=$1"
    },
    {
      "src": "/api/file/(.*)",
      "dest": "/api/file/[file_id].ts?file_id=$1"
    }
  ]
}
